{
  "name": "zyllio-plugin-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run concat && copyfiles -f src/plugin.json dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "concat": "concat -o dist/plugin.js dist/runtime.js dist/polyfills.js dist/main.js",
    "deploy": "lt --port 3000"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/elements": "^13.1.1",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "copyfiles": "^2.4.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "localtunnel": "^2.0.2",
    "@zyllio/zy-sdk": "0.0.10",
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/node": "^12.11.1",
    "concat": "^1.0.3",
    "typescript": "~4.5.2"
  }
}
